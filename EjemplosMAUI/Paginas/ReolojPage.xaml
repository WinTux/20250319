<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="EjemplosMAUI.Paginas.ReolojPage"
             Title="ReolojPage"
             xmlns:dibujable ="clr-namespace:EjemplosMAUI.Elementos">
    <ContentPage.Resources>
        <dibujable:DibujoReloj x:Key="dibujoReloj" />
    </ContentPage.Resources>
    <VerticalStackLayout>
        <GraphicsView x:Name="relojGraphicsView" Drawable="{StaticResource dibujoReloj}"></GraphicsView>
    </VerticalStackLayout>
</ContentPage>